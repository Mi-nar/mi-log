{
    "name": "mi-log",
    "displayName": "Mi log",
    "description": "Mi log is an Extension to create quick log statements.",
    "version": "1.0.3",
    "publisher": "Minar",
    "author": {
        "name": "Minar"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Programming Languages",
        "Debuggers",
        "Other"
    ],
    "keywords": [
        "print",
        "console",
        "logging",
        "mi",
        "mi-log",
        "minar",
        "javascript",
        "typescript",
        "python",
        "php",
        "java",
        "c",
        "cpp",
        "ruby",
        "swift",
        "kotlin",
        "go",
        "rust",
        "csharp",
        "lua",
        "bash",
        "shellscript",
        "matlab",
        "r"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Mi-nar/mi-log"
    },
    "bugs": {
        "url": "https://github.com/Mi-nar/mi-log/issues"
    },
    "homepage": "https://mi-nar.com/",
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript",
        "onLanguage:python",
        "onLanguage:php",
        "onLanguage:java",
        "onLanguage:c",
        "onLanguage:cpp",
        "onLanguage:ruby",
        "onLanguage:swift",
        "onLanguage:kotlin",
        "onLanguage:go",
        "onLanguage:rust",
        "onLanguage:csharp",
        "onLanguage:lua",
        "onLanguage:bash",
        "onLanguage:shellscript",
        "onLanguage:matlab",
        "onLanguage:r"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": false
        }
    },
    "contributes": {
        "languages": [
            {
                "id": "javascript",
                "aliases": [
                    "JavaScript",
                    "javascript",
                    "js",
                    "JS"
                ],
                "extensions": [
                    ".js",
                    ".mjs",
                    ".cjs"
                ]
            },
            {
                "id": "python",
                "aliases": [
                    "Python",
                    "python",
                    "py",
                    "PY"
                ],
                "extensions": [
                    ".py"
                ]
            },
            {
                "id": "java",
                "aliases": [
                    "Java",
                    "java"
                ],
                "extensions": [
                    ".java"
                ]
            },
            {
                "id": "c",
                "aliases": [
                    "C",
                    "c"
                ],
                "extensions": [
                    ".c"
                ]
            },
            {
                "id": "cpp",
                "aliases": [
                    "C++",
                    "cpp"
                ],
                "extensions": [
                    ".cpp",
                    ".cc",
                    ".cxx"
                ]
            },
            {
                "id": "ruby",
                "aliases": [
                    "Ruby",
                    "ruby",
                    "rb",
                    "RB"
                ],
                "extensions": [
                    ".rb",
                    ".rhtml"
                ]
            },
            {
                "id": "php",
                "aliases": [
                    "PHP",
                    "php"
                ],
                "extensions": [
                    ".php",
                    ".php4",
                    ".php5",
                    ".phtml"
                ]
            },
            {
                "id": "swift",
                "aliases": [
                    "Swift",
                    "swift"
                ],
                "extensions": [
                    ".swift"
                ]
            },
            {
                "id": "typescript",
                "aliases": [
                    "TypeScript",
                    "typescript",
                    "ts",
                    "TS"
                ],
                "extensions": [
                    ".ts"
                ]
            },
            {
                "id": "kotlin",
                "aliases": [
                    "Kotlin",
                    "kotlin"
                ],
                "extensions": [
                    ".kt"
                ]
            },
            {
                "id": "go",
                "aliases": [
                    "Go",
                    "go",
                    "Golang",
                    "golang"
                ],
                "extensions": [
                    ".go"
                ]
            },
            {
                "id": "rust",
                "aliases": [
                    "Rust",
                    "rust"
                ],
                "extensions": [
                    ".rs"
                ]
            },
            {
                "id": "csharp",
                "aliases": [
                    "C#",
                    "csharp",
                    "cs",
                    "CS"
                ],
                "extensions": [
                    ".cs"
                ]
            },
            {
                "id": "lua",
                "aliases": [
                    "Lua",
                    "lua"
                ],
                "extensions": [
                    ".lua"
                ]
            },
            {
                "id": "shellscript",
                "aliases": [
                    "Bash",
                    "bash",
                    "ShellScript",
                    "shellscript",
                    "sh",
                    "SH"
                ],
                "extensions": [
                    ".sh"
                ]
            },
            {
                "id": "matlab",
                "aliases": [
                    "Matlab",
                    "matlab"
                ],
                "extensions": [
                    ".m"
                ]
            },
            {
                "id": "r",
                "aliases": [
                    "R",
                    "r"
                ],
                "extensions": [
                    ".r",
                    ".R"
                ]
            }
        ],
        "keybindings": [
            {
                "command": "mi-log",
                "key": "ctrl+shift+l",
                "mac": "cmd+shift+l",
                "when": "editorTextFocus"
            }
        ],
        "commands": [
            {
                "title": "Mi Log",
                "command": "mi-log"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.11.11",
        "@types/vscode": "^1.73.0",
        "typescript": "^4.9.3"
    }
}
