{
    "name": "mi-log",
    "displayName": "Mi log",
    "description": "Mi log is an Extension to create quick log statements.",
    "version": "1.0.0",
    "publisher": "Minar",
    "author": {
        "name": "Minar"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Programming Languages",
        "Debuggers",
        "Other"
    ],
    "keywords": [
        "print",
        "console",
        "logging",
        "mi",
        "mi-log",
        "minar",
        "javascript",
        "typescript",
        "python",
        "php"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/"
    },
    "bugs": {
        "url": "https://github.com/"
    },
    "homepage": "https://github.com/",
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:typescript",
        "onLanguage:python",
        "onLanguage:php"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": false
        }
    },
    "contributes": {
        "languages": [
            {
                "id": "javascript",
                "aliases": ["JavaScript", "javascript", "js"],
                "extensions": [".js"]
            },
            {
                "id": "typescript",
                "aliases": ["TypeScript", "typescript", "ts"],
                "extensions": [".ts"]
            },
            {
                "id": "python",
                "aliases": ["Python", "python", "py"],
                "extensions": [".py"]
            },
            {
                "id": "php",
                "aliases": ["PHP", "php"],
                "extensions": [".php"]
            }
        ],
        "keybindings": [
            {
                "command": "mi-log",
                "key": "ctrl+shift+l",
                "mac": "cmd+shift+l",
                "when": "editorTextFocus"
            }
        ],
        "commands": [
            {
                "title": "Mi Log",
                "command": "mi-log"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.11.11",
        "@types/vscode": "^1.73.0",
        "typescript": "^4.9.3"
    }
}
